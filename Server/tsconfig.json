{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./dist",
		"resolveJsonModule": true,
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strictPropertyInitialization": false
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"src/swagger-output.json",
		"src/**/*.ts"
	],
	"lib": [
		"es2015"
	],
	"typeRoots": [
		"node_modules/@types"
	],
	"types": [
		"node",
		"express"
	]
}